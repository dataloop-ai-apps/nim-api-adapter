{
  "name": "nim-api-niv-22b",
  "displayName": "niv-22b NIM API",
  "version": "0.1.3",
  "scope": "public",
  "description": "Multi-modal vision-language model that understands text/images and generates informative responses\n\n",
  "attributes": {
    "Provider": "Nvidia",
    "Category": "Model",
    "Gen AI": "LMM",
    "NLP": "Conversational"
  },
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/nim-api-adapter",
    "gitTag": "0.1.3"
  },
  "components": {
    "computeConfigs": [
      {
        "name": "niv-22b-deploy",
        "runtime": {
          "podType": "regular-xs",
          "concurrency": 1,
          "runnerImage": "dataloopai/dtlpy-agent:cpu.py3.8.opencv4.7\"",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2
          }
        }
      }
    ],
    "modules": [
      {
        "name": "niv-22b-module",
        "entryPoint": "main.py",
        "className": "ModelAdapter",
        "computeConfig": "niv-22b-deploy",
        "description": "niv-22b NIM API Adapter",
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          },{
            "type": "String",
            "name": "nvidia_api_key_name"
          }
        ],
        "functions": [
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "List of items to run inference on"
              }
            ],
            "output": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The same input images for prediction."
              },
              {
                "type": "Annotation[]",
                "name": "annotations",
                "description": "The predicted annotations."
              }
            ],
            "displayName": "Predict Items",
            "displayIcon": "",
            "description": "NIM API niv-22b predict items"
          }
        ]
      }
    ],
    "models": [
      {
        "name": "nim-niv-22b",
        "moduleName": "niv-22b-module",
        "scope": "project",
        "status": "pre-trained",
        "configuration": {
        },
        "description": "Multi-modal vision-language model that understands text/images and generates informative responses\n\n."
      }
    ]
  }
}