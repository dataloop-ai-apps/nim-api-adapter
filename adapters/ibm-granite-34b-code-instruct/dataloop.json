{
  "name": "nim-api-ibm-granite-34b-code-instruct",
  "displayName": "IBM granite-34b",
  "version": "0.1.9",
  "scope": "project",
  "description": "ibm granite 34b from NIM API",
  "attributes": {
    "Hub": "Nvidia",
    "Provider": "Nvidia",
    "Category": "Model",
    "Gen AI": "LMM",
    "NLP": "Conversational"
  },
  "components": {
    "computeConfigs": [
      {
        "name": "granite-34b-nim-deploy",
        "runtime": {
          "podType": "regular-xs",
          "concurrency": 10,
          "runnerImage": "dataloopai/dtlpy-agent:cpu.py3.8.opencv4.7",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 1,
            "maxReplicas": 2
          }
        }
      }
    ],
    "modules": [
      {
        "name": "granite-34b-nim-module",
        "entryPoint": "main.py",
        "className": "ModelAdapter",
        "computeConfig": "granite-34b-nim-deploy",
        "description": "granite-34b NIM API Adapter",
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          },
          {
            "type": "String",
            "name": "nvidia_api_key_name"
          }
        ],
        "functions": [
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "List of items to run inference on"
              }
            ],
            "output": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The same input images for prediction."
              },
              {
                "type": "Annotation[]",
                "name": "annotations",
                "description": "The predicted annotations."
              }
            ],
            "displayName": "Predict Items",
            "displayIcon": "",
            "description": "NIM API granite-34b predict items"
          }
        ]
      }
    ],
    "models": [
      {
        "name": "granite-34b-nim",
        "moduleName": "granite-34b-nim-module",
        "scope": "project",
        "status": "pre-trained",
        "configuration": {
          "max_tokens": 1024,
          "temperature": 0.2,
          "top_p": 0.70,
          "seed": 0
        },
        "description": "IBM granite NIM API"
      }
    ]
  }
}