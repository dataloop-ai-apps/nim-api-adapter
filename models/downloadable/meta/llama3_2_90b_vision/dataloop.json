{
  "name": "nim-llama3-2-90b-vision-meta",
  "displayName": "llama3-2-90b-vision",
  "version": "0.3.24",
  "scope": "public",
  "description": "Downloadable meta/llama3-2-90b-vision from NIM",
  "attributes": {
    "Hub": ["Nvidia", "Dataloop"],
    "Provider": "Meta",
    "Deployed By": "Dataloop",
    "Category": ["Model", "NIM"],
    "Gen AI": "LLM",
    "Media Type": ["Text"],
    "NLP": "Conversational"
  },
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/nim-api-adapter",
    "gitTag": "0.3.24"
  },
  "components": {
    "computeConfigs": [
      {
        "name": "nim-downloadable-deploy",
        "runtime": {
          "podType": "gpu-a100-s",
          "concurrency": 10,
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/gpu/llama3-2-90b-vision:0.1.5",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2
          }
        }
      }
    ],
    "modules": [
      {
        "name": "nim-downloadable-module",
        "entryPoint": "models/downloadable/main_downloadable_adapter.py",
        "className": "ModelAdapter",
        "computeConfig": "nim-downloadable-deploy",
        "integrations": ["dl-ngc-api-key"],
        "description": "Downloadable llama3 NIM Adapter module",
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "List of items to run inference on"
              }
            ],
            "output": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The same input images for prediction."
              },
              {
                "type": "Annotation[]",
                "name": "annotations",
                "description": "The predicted annotations."
              }
            ],
            "displayName": "Predict Items",
            "displayIcon": "",
            "description": "NIM llama3 predict items"
          }
        ]
      }
    ],
    "models": [
      {
        "name": "llama3-2-90b-vision",
        "moduleName": "nim-downloadable-module",
        "scope": "project",
        "status": "pre-trained",
        "configuration": {
          "nim_model_name": "meta/llama3-2-90b-vision",
          "system_prompt": "You are a helpful and a bit cynical assistant. Give relevant and short answers, if you dont know the answer just say it, dont make up an answer",
          "max_tokens": 1024,
          "temperature": 0.5,
          "top_p": 1,
          "seed": 0
        },
        "description": "Downloadable meta/llama3-2-90b-vision from NIM"
      }
    ]
  }
}
