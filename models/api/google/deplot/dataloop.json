{
  "name": "google-deplot",
  "displayName": "google-deplot",
  "version": "0.3.11",
  "scope": "public",
  "description": "Google DePlot, a one-shot solution for visual language understanding, converting images of plots or charts into structured tables, served by NIM API.",
  "attributes": { 
    "Hub": [
      "Nvidia",
      "Dataloop"
    ],
    "Provider": "Google",
    "Deployed By": "NVIDIA",
    "Category": [
      "Model",
      "NIM"
    ],
    "Gen AI": "LMM",
    "Media Type": [
      "Image",
      "Text",
      "Multi Modal"
    ],
    "NLP": "Conversational"
  },
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/nim-api-adapter",
    "gitTag": "0.3.11"
  },
  "components": {
    "computeConfigs": [
      {
        "name": "nim-api-deploy",
        "runtime": {
          "podType": "regular-xs",
          "concurrency": 1,
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/cpu/nim-api:0.1.10",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2
          }
        }
      }
    ],
    "modules": [
      {
        "name": "nim-api-module",
        "entryPoint": "models/api/meta/llama_api_adapter.py",
        "className": "LlamaAdapter",
        "computeConfig": "nim-api-deploy",
        "description": "NIM API Adapter for Google DePlot, facilitating the conversion of plot or chart images into structured tables.",
        "integrations": [
          "dl-ngc-api-key"
        ],
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "List of items to run inference on"
              }
            ],
            "output": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The same input images for prediction."
              },
              {
                "type": "Annotation[]",
                "name": "annotations",
                "description": "The predicted annotations."
              }
            ],
            "displayName": "Predict Items",
            "displayIcon": "",
            "description": "Predict items using Google DePlot via NIM API."
          }
        ]
      }
    ],
    "models": [
      {
        "name": "google-deplot",
        "moduleName": "nim-api-module",
        "scope": "project",
        "status": "pre-trained",
        "configuration": {
          "nim_invoke_url": "vlm/google/deplot",
          "nim_model_name": "google/deplot",
          "max_tokens": 1024,
          "temperature": 0.20,
          "top_p": 0.20,
          "stream": true
        },
        "description": "Google DePlot model for transforming plot or chart images into structured tables, served by NIM API."
      }
    ]
  }
}