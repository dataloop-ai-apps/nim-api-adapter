{
  "name": "nim-nv-embed-v1",
  "displayName": "Nv Embed V1",
  "version": "0.3.49",
  "scope": "public",
  "description": "NVIDIA NIM adapter for nvidia/nv-embed-v1",
  "attributes": {
    "Category": [
      "Model",
      "NIM"
    ],
    "NLP": "Embeddings",
    "Provider": "NVIDIA",
    "Media Type": [
      "Text"
    ],
    "Hub": [
      "Nvidia",
      "Dataloop"
    ],
    "Deployed By": "NVIDIA"
  },
  "components": {
    "computeConfigs": [
      {
        "name": "nim-nv-embed-v1-deploy",
        "runtime": {
          "podType": "regular-xs",
          "concurrency": 1,
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 100
          },
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/apps/nim-api-adapter:0.3.43"
        }
      }
    ],
    "modules": [
      {
        "name": "nim-nv-embed-v1-module",
        "entryPoint": "models/api/embeddings/base.py",
        "className": "ModelAdapter",
        "computeConfig": "nim-nv-embed-v1-deploy",
        "description": "NVIDIA NIM adapter for nvidia/nv-embed-v1",
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "embed_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The input items for embeddings."
              }
            ],
            "output": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The same input items for embeddings."
              },
              {
                "type": "Json",
                "name": "json",
                "description": "Embeddings of items."
              }
            ],
            "displayName": "Embed Items",
            "displayIcon": "",
            "description": "The inference function of the model."
          },
          {
            "name": "embed_dataset",
            "input": [
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "The input dataset of the items required for prediction."
              },
              {
                "type": "Json",
                "name": "filters",
                "description": "The DQL in json format to get all the items required for prediction."
              }
            ],
            "output": [],
            "displayName": "Embed Dataset",
            "displayIcon": "",
            "description": "Inference function of the model on a dataset."
          }
        ],
        "integrations": [
          "dl-ngc-api-key"
        ]
      }
    ],
    "models": [
      {
        "name": "nim-nv-embed-v1",
        "moduleName": "nim-nv-embed-v1-module",
        "scope": "project",
        "status": "pre-trained",
        "inputType": "text",
        "description": "NVIDIA NIM adapter for nvidia/nv-embed-v1",
        "configuration": {
          "nim_model_name": "nvidia/nv-embed-v1",
          "embeddings_size": 4096,
          "hyde_model_name": "",
          "base_url": "https://integrate.api.nvidia.com/v1"
        }
      }
    ]
  },
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/nim-api-adapter",
    "gitTag": "0.3.49"
  }
}
