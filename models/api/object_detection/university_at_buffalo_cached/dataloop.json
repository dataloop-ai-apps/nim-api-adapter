{
  "name": "university-at-buffalo-cached",
  "displayName": "university-at-buffalo-cached",
  "version": "0.3.56",
  "scope": "public",
  "description": "CACHED, a chart element detection model from University at Buffalo, served by NIM API.",
  "attributes": {
    "Hub": ["Nvidia", "Dataloop"],
    "Provider": "Other",
    "Deployed By": "NVIDIA",
    "Category": ["Model", "NIM"],
    "Computer Vision": "Object Detection",
    "Media Type": ["Image"]
  },
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/nim-api-adapter",
    "gitTag": "0.3.56"
  },
  "components": {
    "computeConfigs": [
      {
        "name": "nim-api-deploy",
        "runtime": {
          "podType": "regular-xs",
          "concurrency": 1,
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/apps/nim-api-adapter:0.3.43",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2
          }
        }
      }
    ],
    "modules": [
      {
        "name": "nim-api-module",
        "entryPoint": "models/api/object_detection/base.py",
        "className": "ModelAdapter",
        "computeConfig": "nim-api-deploy",
        "description": "NIM API Adapter for CACHED model detecting chart elements.",
        "integrations": ["dl-ngc-api-key"],
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "List of items to run inference on"
              }
            ],
            "output": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The same input images for prediction."
              },
              {
                "type": "Annotation[]",
                "name": "annotations",
                "description": "The predicted annotations."
              }
            ],
            "displayName": "Predict Items",
            "displayIcon": "",
            "description": "Predict items using CACHED model via NIM API."
          }
        ]
      }
    ],
    "models": [
      {
        "name": "university-at-buffalo-cached",
        "moduleName": "nim-api-module",
        "scope": "project",
        "status": "pre-trained",
        "configuration": {
          "nim_model_name": "cv/university-at-buffalo/cached"
        },
        "description": "CACHED model for chart element detection, served by NIM API."
      }
    ]
  }
}
